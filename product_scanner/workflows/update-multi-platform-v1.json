{
  "defaults": {
    "platform": "multi_platform",
    "product_id": "",
    "sale_status": ""
  },
  "description": "product_id 기반 멀티 플랫폼 상품 추출 + DB 업데이트 (검증 후 변경 감지 항목 자동 업데이트)",
  "metadata": {
    "author": "Product Scanner Team",
    "created_at": "2025-11-25",
    "tags": [
      "extract",
      "update",
      "multi-platform",
      "phase2"
    ]
  },
  "name": "Multi-Platform Extract + Update",
  "nodes": {
    "1": {
      "config": {
        "product_id": "${product_id}",
        "sale_status": "${sale_status}"
      },
      "name": "Extract Products by ProductId (Multi-Platform)",
      "next_nodes": [
        "2"
      ],
      "retry": {
        "backoff_ms": 2000,
        "max_attempts": 2
      },
      "timeout_ms": 300000,
      "type": "extract_multi_platform"
    },
    "2": {
      "config": {},
      "name": "Update Product Sets in Supabase",
      "next_nodes": [],
      "retry": {
        "backoff_ms": 1000,
        "max_attempts": 3
      },
      "timeout_ms": 60000,
      "type": "update_product_set"
    }
  },
  "start_node": "1",
  "version": "1.0.0",
  "workflow_id": "update-multi-platform-v1"
}
