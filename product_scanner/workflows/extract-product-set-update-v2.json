{
  "defaults": {
    "platform": "productset",
    "product_set_id": "",
    "update_sale_status": true
  },
  "description": "ProductSet ID 상품 추출 + Supabase 업데이트",
  "metadata": {
    "author": "Product Scanner Team",
    "created_at": "2025-11-27",
    "tags": [
      "extract",
      "update",
      "product-set",
      "typed-node"
    ]
  },
  "name": "Extract & Update Product by ProductSet ID",
  "nodes": {
    "extract": {
      "config": {
        "output_dir": "/app/results",
        "product_set_id": "${product_set_id}"
      },
      "name": "ProductSet 기반 상품 추출",
      "next_nodes": [
        "update"
      ],
      "retry": {
        "backoff_ms": 2000,
        "max_attempts": 2
      },
      "timeout_ms": 60000,
      "type": "extract_product_set"
    },
    "update": {
      "config": {
        "platform": "${platform}",
        "record_history": true,
        "update_sale_status": "${update_sale_status}",
        "verify_updates": true
      },
      "name": "Supabase 상품 업데이트",
      "next_nodes": [
        "notify"
      ],
      "timeout_ms": 300000,
      "type": "update_product_set"
    },
    "notify": {
      "config": {
        "notify_on_failure": true,
        "slack_enabled": true
      },
      "name": "Slack 알림",
      "next_nodes": [],
      "type": "notify_result"
    }
  },
  "start_node": "extract",
  "version": "2.0.0",
  "workflow_id": "extract-product-set-update-v2"
}
