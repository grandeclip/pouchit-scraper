{
  "description": "ProductSet ID 상품 추출 (Supabase 조회 + DB 비교)",
  "metadata": {
    "author": "Product Scanner Team",
    "created_at": "2025-11-26",
    "tags": [
      "extract",
      "validation",
      "product-set",
      "typed-node"
    ]
  },
  "name": "Extract Product by ProductSet ID",
  "nodes": {
    "extract": {
      "config": {
        "output_dir": "/app/results",
        "product_set_id": "${product_set_id}"
      },
      "name": "ProductSet 기반 상품 추출",
      "next_nodes": [
        "notify"
      ],
      "retry": {
        "backoff_ms": 2000,
        "max_attempts": 2
      },
      "timeout_ms": 60000,
      "type": "extract_product_set"
    },
    "notify": {
      "config": {
        "notify_on_failure": true,
        "slack_enabled": true
      },
      "name": "Slack 알림",
      "next_nodes": [],
      "type": "notify_result"
    }
  },
  "start_node": "extract",
  "version": "2.0.0",
  "workflow_id": "extract-product-set-validation-v2"
}