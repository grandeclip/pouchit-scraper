{
  "defaults": {
    "platform": "url_extraction",
    "url": ""
  },
  "description": "URL 상품 추출 (Supabase 조회 없음, db/comparison: null)",
  "metadata": {
    "author": "Product Scanner Team",
    "created_at": "2025-11-26",
    "tags": [
      "extract",
      "url",
      "validation",
      "typed-node"
    ]
  },
  "name": "Extract Product by URL",
  "nodes": {
    "extract": {
      "config": {
        "output_dir": "/app/results",
        "url": "${url}"
      },
      "name": "URL 기반 상품 추출",
      "next_nodes": [
        "notify"
      ],
      "retry": {
        "backoff_ms": 2000,
        "max_attempts": 2
      },
      "timeout_ms": 60000,
      "type": "extract_url"
    },
    "notify": {
      "config": {
        "notify_on_failure": true,
        "slack_enabled": true
      },
      "name": "Slack 알림",
      "next_nodes": [],
      "type": "notify_result"
    }
  },
  "start_node": "extract",
  "version": "2.0.0",
  "workflow_id": "extract-url-validation-v2"
}
